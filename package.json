{
  "name": "sprintjam",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "CLOUDFLARE_ENV=development vite",
    "prebuild": "tsc -b",
    "build": "vite build",
    "build:staging": "CLOUDFLARE_ENV=staging vite build",
    "typecheck": "tsc --noEmit",
    "format": "npx prettier --write .",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "PLAYWRIGHT_RUN_SMOKE=1 pnpm test:e2e",
    "test:e2e:smoke:staging": "PLAYWRIGHT_RUN_SMOKE=1 E2E_BASE_URL=https://staging.sprintjam.co.uk pnpm test:e2e",
    "test:e2e:smoke:production": "PLAYWRIGHT_RUN_SMOKE=1 E2E_BASE_URL=https://sprintjam.co.uk pnpm test:e2e",
    "test:e2e:all": "PLAYWRIGHT_RUN_A11Y=1 pnpm test:e2e",
    "test:e2e:a11y": "PLAYWRIGHT_RUN_A11Y=1 pnpm test:e2e tests/e2e/a11y",
    "deploy": "npm run build && wrangler deploy",
    "deploy:staging": "npm run build:staging && wrangler deploy",
    "db:generate": "drizzle-kit generate"
  },
  "dependencies": {
    "@strudel/web": "1.2.6",
    "@tailwindcss/typography": "0.5.19",
    "@tanstack/db": "0.4.10",
    "@tanstack/query-core": "5.90.12",
    "@tanstack/query-db-collection": "0.2.31",
    "@tanstack/react-query": "5.90.12",
    "@tanstack/react-query-devtools": "5.90.2",
    "canvas-confetti": "1.9.4",
    "drizzle-orm": "0.45.1",
    "framer-motion": "12.23.26",
    "lucide-react": "0.545.0",
    "qrcode.react": "4.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwindcss": "4.1.14",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@cloudflare/vite-plugin": "1.13.13",
    "@cloudflare/workers-types": "4.20251209.0",
    "@playwright/test": "1.56.1",
    "@tailwindcss/vite": "4.1.14",
    "@types/node": "24.10.3",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/ui": "4.0.9",
    "drizzle-kit": "0.31.8",
    "typescript": "5.9.3",
    "vite": "7.2.7",
    "vitest": "4.0.9",
    "wrangler": "4.53.0"
  }
}
