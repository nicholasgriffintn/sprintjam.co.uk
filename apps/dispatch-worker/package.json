{
  "name": "@sprintjam/dispatch-worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./api/index.ts",
  "scripts": {
    "dev": "CLOUDFLARE_ENV=development vite",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "PLAYWRIGHT_RUN_SMOKE=1 pnpm test:e2e",
    "test:e2e:smoke:staging": "PLAYWRIGHT_RUN_SMOKE=1 E2E_BASE_URL=https://staging.sprintjam.co.uk pnpm test:e2e",
    "test:e2e:smoke:production": "PLAYWRIGHT_RUN_SMOKE=1 E2E_BASE_URL=https://sprintjam.co.uk pnpm test:e2e",
    "test:e2e:all": "PLAYWRIGHT_RUN_A11Y=1 pnpm test:e2e",
    "test:e2e:a11y": "PLAYWRIGHT_RUN_A11Y=1 pnpm test:e2e tests/e2e/a11y",
    "build": "vite build",
    "build:staging": "CLOUDFLARE_ENV=staging vite build",
    "deploy": "wrangler deploy",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:prod": "wrangler deploy --env production",
    "typecheck": "tsc --noEmit",
    "generate:types": "wrangler types --include-runtime=false",
    "preview": "vite preview"
  },
  "dependencies": {
    "@sprintjam/types": "workspace:*",
    "@strudel/web": "1.2.6",
    "@tailwindcss/typography": "0.5.19",
    "@tanstack/db": "0.4.10",
    "@tanstack/query-core": "5.90.12",
    "@tanstack/query-db-collection": "0.2.31",
    "@tanstack/react-query": "5.90.12",
    "@tanstack/react-query-devtools": "5.90.2",
    "canvas-confetti": "1.9.4",
    "framer-motion": "12.23.26",
    "lucide-react": "0.545.0",
    "qrcode.react": "4.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwindcss": "4.1.14",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.20.0",
    "@cloudflare/workers-types": "4.20260107.1",
    "@playwright/test": "1.57.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.2",
    "typescript": "5.9.3",
    "vite": "7.2.7",
    "vitest": "4.0.16",
    "wrangler": "4.57.0"
  }
}
