{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "sprintjam",
	"main": "server/index.ts",
	"compatibility_date": "2025-04-10",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"durable_objects": {
		"bindings": [
			{ "name": "POKER_ROOM", "class_name": "PokerRoom" }
		]
	},
	"migrations": [
		{ "tag": "v1", "new_classes": ["PokerRoom"] }
	],
	"assets": {
		"binding": "ASSETS",
		"directory": "./app"
	},
	"observability": {
		"enabled": false
	}
}
